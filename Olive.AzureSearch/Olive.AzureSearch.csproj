<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Title>Olive.AzureSearch</Title>
    <Version>1.0.1</Version>
    <Authors>Geeks Ltd</Authors>
    <Product>$(AssemblyName) ($(TargetFramework))</Product>
    <Description>Olive Azure Search</Description>
    <Copyright>Geeks Ltd 2022, All rights reserved.</Copyright>
    <PackageProjectUrl>https://geeksltd.github.io/Olive</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Geeksltd/Olive</RepositoryUrl>
    <PackageTags>olive azure search azuresearch</PackageTags>
    <NeutralLanguage>en</NeutralLanguage>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Search.Documents" Version="11.4.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Olive\Olive.csproj" />
  </ItemGroup>

</Project>
